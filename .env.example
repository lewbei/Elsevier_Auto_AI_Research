ELSEVIER_KEY=your_elsevier_key
DEEPSEEK_API_KEY=your_deepseek_api_key
OPENAI_API_KEY=your_openai_api_key
HUGGINGFACE_HUB_TOKEN=your_huggingface_access_token  # optional; needed for google/embeddinggemma-300m embeddings
# X_ELS_INSTTOKEN=your_elsevier_institution_token  # optional - enables access from any IP address

# Optional pipeline toggles (uncomment/set as needed)
# WRITE_PAPER=1
# MAX_ITERS=2
# SKIP_FIND_PAPERS=1
# SKIP_NOVELTY=1
# SKIP_PLANNER=1
# SKIP_ITERATE=1

# Verbose logging and LLM tracing
# LOG_LEVEL=debug   # debug|info|warn|error (debug prints detailed progress)
# VERBOSE=1         # legacy alias for LOG_LEVEL=debug
# LLM_LOG=1         # write LLM payload/response JSON to logs/llm/
# LLM_COST_INPUT_PER_1K=0.002   # USD per 1K input tokens (optional)
# LLM_COST_OUTPUT_PER_1K=0.008  # USD per 1K output tokens (optional)
